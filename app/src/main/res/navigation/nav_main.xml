<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/onBoardingContainerFragment"
        android:name="com.example.tamangfood.ui.onboarding.OnBoardingContainerFragment"
        android:label="fragment_on_boarding_container"
        tools:layout="@layout/fragment_on_boarding_container">
        <action
            android:id="@+id/action_onBoardingContainerFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.tamangfood.ui.welcome.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome">
        <action
            android:id="@+id/action_welcomeFragment_to_onBoardingContainerFragment"
            app:destination="@id/onBoardingContainerFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.tamangfood.ui.signin.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in">
        <action
            android:id="@+id/action_signInFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_signInFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim"
            app:popUpTo="@id/nav_main"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.example.tamangfood.ui.forgotpassword.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" />
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.tamangfood.ui.signup.SignUpFragment"
        android:label="SignUpFragment"
        tools:layout="@layout/fragment_sign_up" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.tamangfood.ui.homepage.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_featuredPartnersFragment"
            app:destination="@id/featuredPartnersFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_bestPickFragment"
            app:destination="@id/bestPickFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />

    </fragment>
    <fragment
        android:id="@+id/accountSettingsFragment"
        android:name="com.example.tamangfood.ui.accountsettings.AccountSettingsFragment"
        android:label="fragment_account_settings"
        tools:layout="@layout/fragment_account_settings">
        <action
            android:id="@+id/action_accountSettingsFragment_to_profileSettingsFragment"
            app:destination="@id/profileSettingsFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_accountSettingsFragment_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_accountSettingsFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popUpTo="@id/signInFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/ordersFragment"
        android:name="com.example.tamangfood.ui.orders.OrdersFragment"
        android:label="fragment_orders"
        tools:layout="@layout/fragment_orders" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.tamangfood.ui.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/action_searchFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
        <action
            android:id="@+id/action_searchFragment_to_searchChildFragment"
            app:destination="@id/searchChildFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/singleRestaurantFragment"
        android:name="com.example.tamangfood.ui.singlerestaurent.SingleRestaurantFragment"
        android:label="fragment_single_restaurant"
        tools:layout="@layout/fragment_single_restaurant">

        <action
            android:id="@+id/action_singleRestaurantFragment_to_addToOrdersFragment"
            app:destination="@id/addToOrdersFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/featuredPartnersFragment"
        android:name="com.example.tamangfood.ui.featuredpartners.FeaturedPartnersFragment"
        android:label="fragment_featured_partners"
        tools:layout="@layout/fragment_featured_partners">
        <action
            android:id="@+id/action_featuredPartnersFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/bestPickFragment"
        android:name="com.example.tamangfood.ui.bestpick.BestPickFragment"
        android:label="BestPickFragment"
        tools:layout="@layout/fragment_best_pick">
        <action
            android:id="@+id/action_bestPickFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/profileSettingsFragment"
        android:name="com.example.tamangfood.ui.accountsettings.profilesettings.ProfileSettingsFragment"
        android:label="fragment_profile_settings"
        tools:layout="@layout/fragment_profile_settings">
        <action
            android:id="@+id/action_profileSettingsFragment_to_changePasswordFragment"
            app:destination="@id/changePasswordFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.example.tamangfood.ui.accountsettings.profilesettings.changepassword.ChangePasswordFragment"
        android:label="ChangePasswordFragment"
        tools:layout="@layout/fragment_change_password">
        <action
            android:id="@+id/action_changePasswordFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/addToOrdersFragment"
        android:name="com.example.tamangfood.ui.singlerestaurent.addtoorders.AddToOrdersFragment"
        android:label="AddToOrdersFragment"
        tools:layout="@layout/fragment_add_to_orders">
        <action
            android:id="@+id/action_addToOrdersFragment_to_yourOrdersFragment"
            app:destination="@id/yourOrdersFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/seeFoodFragment"
        android:name="com.example.tamangfood.ui.singlerestaurent.typefood.SeeFoodFragment"
        android:label="fragment_see_food"
        tools:layout="@layout/fragment_see_food">
        <action
            android:id="@+id/action_seeFoodFragment_to_addToOrdersFragment"
            app:destination="@id/addToOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/appetizersFragment"
        android:name="com.example.tamangfood.ui.singlerestaurent.typefood.AppetizersFragment"
        android:label="AppetizersFragment" />
    <fragment
        android:id="@+id/yourOrdersFragment"
        android:name="com.example.tamangfood.ui.yourorders.YourOrdersFragment"
        android:label="fragment_your_orders"
        tools:layout="@layout/fragment_your_orders">
        <action
            android:id="@+id/action_yourOrdersFragment_to_addPaymentFragment"
            app:destination="@id/addPaymentFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim" />
        <action
            android:id="@+id/action_yourOrdersFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment" />
    </fragment>
    <fragment
        android:id="@+id/addPaymentFragment"
        android:name="com.example.tamangfood.ui.addpayment.AddPaymentFragment"
        android:label="fragment_add_payment"
        tools:layout="@layout/fragment_add_payment">
        <action
            android:id="@+id/action_addPaymentFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/paymentMethodFragment"
        android:name="com.example.tamangfood.ui.payment.PaymentMethodFragment"
        android:label="fragment_payment_method"
        tools:layout="@layout/fragment_payment_method">
        <action
            android:id="@+id/action_paymentMethodFragment_to_myPaymentMethodsFragment"
            app:destination="@id/myPaymentMethodsFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/myPaymentMethodsFragment"
        android:name="com.example.tamangfood.ui.payment.MyPaymentMethodsFragment"
        android:label="fragment_my_payment_methods"
        tools:layout="@layout/fragment_my_payment_methods">

        <action
            android:id="@+id/action_myPaymentMethodsFragment_to_addPaymentFragment"
            app:destination="@id/addPaymentFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/searchChildFragment"
        android:name="com.example.tamangfood.ui.search.searchchild.SearchChildFragment"
        android:label="fragment_search_child"
        tools:layout="@layout/fragment_search_child">
        <action
            android:id="@+id/action_searchChildFragment_to_resultSearchingFragment"
            app:destination="@id/resultSearchingFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim"
            app:popEnterAnim="@anim/pop_enter_anim"
            app:popExitAnim="@anim/pop_axit_anim" />
    </fragment>
    <fragment
        android:id="@+id/resultSearchingFragment"
        android:name="com.example.tamangfood.ui.search.resultsearching.ResultSearchingFragment"
        android:label="ResultSearchingFragment"
        tools:layout="@layout/fragment_result_searching">
        <action
            android:id="@+id/action_resultSearchingFragment_to_singleRestaurantFragment"
            app:destination="@id/singleRestaurantFragment"
            app:enterAnim="@anim/enter_anim"
            app:exitAnim="@anim/exit_anim" />
    </fragment>

</navigation>